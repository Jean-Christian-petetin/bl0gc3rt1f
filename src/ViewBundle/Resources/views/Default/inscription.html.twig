{%extends "base.html.twig"%}

{% block title %}
    {{ parent() }}
    - Inscription
    {% endblock %}
{% block body %}
<h1>INSCRIPTION</h1>

<div>
    {{ form_start(formAddUser,{'attr':{'action':path('inscription')}})}}
        {{ form_row(formAddUser.nom) }}
        {{ form_row(formAddUser.prenom) }}
        {{ form_row(formAddUser.email) }}
        {{ form_row(formAddUser.motdepasse) }}
        {{ form_row(formAddUser.avatar) }}
        <input type="submit" value="Valider" />
</div>

{% endblock %}